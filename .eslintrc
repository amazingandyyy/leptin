{
  "env": {
    "commonjs": true,
    "es6": true,
    "node": true,
    "jest/globals": true
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "extends": [
    "standard",
    "eslint:recommended",
    "airbnb"
  ],
  "rules": {
    "import/prefer-default-export": 0,
    "no-console": 0,
    "no-extra-semi": "error",
    "quotes": [2, "single"],
    "no-shadow": 0,
    "max-len": 0,
    "no-mixed-operators": 0,
    "no-bitwise": 0,
    "no-underscore-dangle": 0,
    "arrow-parens": 0,
    "eol-last": 0,
    "consistent-return": 0,
    "comma-dangle": 0,
    "object-curly-newline": 0,
    "no-param-reassign": 0,
    "func-names": 0,
    "no-plusplus": 0,
    "class-methods-use-this": 0
  },
  "plugins": [
    "node",
    "jest"
  ]
}
